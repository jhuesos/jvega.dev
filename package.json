{
  "name": "jvega.dev",
  "version": "0.0.0",
  "description": "jvega.dev website",
  "main": "src/index.js",
  "repository": "https://github.com/jhuesos/jvega.dev",
  "author": "Jaime Vega",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "build:analyze": "ANALYZE_BUNDLE=true webpack --config webpack.config.prod.js",
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "commit": "git-cz",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js .",
    "lint:prettier": "prettier \"**/*.js\" --list-different || (echo '↑↑ these files are not prettier formatted ↑↑' && exit 1)",
    "lint:markdown": "remark -q .",
    "lint:style": "stylelint src/**/*.css",
    "format": "run-s lint:*",
    "format:eslint": "eslint --ext .js . --fix",
    "format:prettier": "prettier \"**/*.js\" --write",
    "analyze-build-bundle": "webpack --config webpack.config.prod.js --profile --json > .tmp/stats.json && webpack-bundle-analyzer .tmp/stats.json",
    "preverify": "npm run build",
    "verify": "npm run verify:bundlesize",
    "verify:bundlesize": "bundlesize",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "defaults"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "resolutions": {
    "optimize-css-assets-webpack-plugin/**/minimist": "^0.2.1",
    "commitizen/**/lodash": "^4.17.9",
    "@commitlint/config-conventional/**/dot-prop": "^4.2.1",
    "optimize-css-assets-webpack-plugin/**/lodash": "^4.17.9",
    "webpack-dev-server/http-proxy-middleware/http-proxy": "^1.18.1",
    "postcss-custom-properties/**/is-url-superb": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@commitlint/cli": "^9.0.0",
    "@commitlint/config-conventional": "^9.0.0",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "bundlesize": "^0.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.1.2",
    "copy-webpack-plugin": "^6.0.0",
    "css-loader": "^4.2.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "mini-css-extract-plugin": "^0.10.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-custom-properties": "^9.1.1",
    "postcss-loader": "^3.0.0",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^2.0.5",
    "remark-cli": "^8.0.0",
    "remark-preset-lint-recommended": "^4.0.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.3.3",
    "stylelint-config-recommended": "^3.0.0",
    "svg-url-loader": "^6.0.0",
    "terser-webpack-plugin": "^4.1.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.2",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "hookrouter": "^1.2.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "regenerator-runtime": "^0.13.7"
  }
}
