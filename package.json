{
  "name": "jvega.dev",
  "version": "0.0.0",
  "description": "jvega.dev website",
  "main": "src/index.js",
  "repository": "https://github.com/jhuesos/jvega.dev",
  "author": "Jaime Vega",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "build:analyze": "ANALYZE_BUNDLE=true webpack --config webpack.config.prod.js",
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "commit": "commit",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js .",
    "lint:prettier": "prettier \"**/*.js\" --list-different || (echo '↑↑ these files are not prettier formatted ↑↑' && exit 1)",
    "lint:markdown": "remark -q .",
    "lint:style": "stylelint src/**/*.css",
    "format": "run-s lint:*",
    "format:eslint": "eslint --ext .js . --fix",
    "format:prettier": "prettier \"**/*.js\" --write",
    "analyze-build-bundle": "webpack --config webpack.config.prod.js --profile --json > .tmp/stats.json && webpack-bundle-analyzer .tmp/stats.json",
    "preverify": "npm run build",
    "verify": "npm run verify:bundlesize",
    "verify:bundlesize": "bundlesize",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "defaults"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@commitlint/prompt-cli": "8.3.5",
    "@testing-library/jest-dom": "5.5.0",
    "@testing-library/react": "10.0.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.4.0",
    "babel-loader": "8.1.0",
    "bundlesize": "0.18.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.5.3",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.2.0",
    "husky": "4.2.5",
    "jest": "25.4.0",
    "lint-staged": "10.1.7",
    "mini-css-extract-plugin": "0.9.0",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-custom-properties": "9.1.1",
    "postcss-loader": "3.0.0",
    "preload-webpack-plugin": "3.0.0-beta.4",
    "prettier": "2.0.5",
    "remark-cli": "8.0.0",
    "remark-preset-lint-recommended": "4.0.0",
    "style-loader": "1.2.0",
    "stylelint": "13.3.3",
    "stylelint-config-recommended": "3.0.0",
    "svg-url-loader": "5.0.0",
    "terser-webpack-plugin": "2.3.5",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2",
    "workbox-webpack-plugin": "4.3.1"
  },
  "dependencies": {
    "core-js": "3.6.5",
    "hookrouter": "1.2.3",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "regenerator-runtime": "0.13.5"
  }
}
